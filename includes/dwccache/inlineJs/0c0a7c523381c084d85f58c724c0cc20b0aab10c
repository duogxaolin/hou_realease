s:420:"<script type="text/javascript">$("#forgot").on("click",function(){$('#forgot').html('<i class="fa fa-spinner fa-spin"></i> Đang Xử Lý').prop('disabled',true);$.ajax({url:"https://localhost/hou_realease/ajaxs/forgot.php",method:"POST",data:{email:$("#email").val(),type:$("#type").val()},success:function(response){$("#thongbao").html(response);$('#forgot').html('Xác nhận').prop('disabled',false);}});});</script>";